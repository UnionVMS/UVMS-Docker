/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/admin-objects=UVMSReportingEvent:write-attribute(name=use-java-context, value=true)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/admin-objects=UVMSReportingEvent/config-properties=PhysicalName/:add(value=UVMSReportingEvent)

