set resourceAdapterFilePath=${resource-adapter-file-path}
set resourceAdapterFile=${resource-adapter-file}
set resourceAdapterName=${resource-adapter-name}
set activemqURL=${activemq-url}
set activemqUser=${activemq-user}
set activemqPassword=${activemq-password}

batch

# ---------------------------------------------
#
# NOT REQUIRED because part of EC configuration
#
# deploying rar file
#deploy $resourceAdapterFilePath --enabled
#
# ---------------------------------------------

# ---------------------------------------------
# Add resource adapter
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName:add(archive=$resourceAdapterFile, transaction-support=XATransaction) 

# add resource adapter properties, issue on appending ?jms.rmIdFromConnectionId=true&amp;socketBufferSize=131072
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/config-properties=UserName/:add(value=$activemqUser)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/config-properties=Password/:add(value=$activemqPassword)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/config-properties=ServerUrl/:add(value=$activemqURL)

# Add resource adapter properties
# Please look at attributes at https://access.redhat.com/documentation/en-us/jboss_enterprise_application_platform/6/html/administration_and_configuration_guide/resource_adapter_descriptor_reference1

/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory:add(class-name=org.apache.activemq.ra.ActiveMQManagedConnectionFactory, jndi-name=java:/ConnectionFactory,enabled=true)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory:write-attribute(name=min-pool-size,value=8)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory:write-attribute(name=max-pool-size,value=64)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory:write-attribute(name=pool-prefill,value=false)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory:write-attribute(name=same-rm-override,value=false)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory/:write-attribute(name=recovery-username, value=$activemqUser)
/subsystem=resource-adapters/resource-adapter=$resourceAdapterName/connection-definitions=ConnectionFactory/:write-attribute(name=recovery-password, value=$activemqPassword)
# ---------------------------------------------

# ---------------------------------------------
# Adding MDB capability
/subsystem=ejb3:write-attribute(name="default-mdb-instance-pool", value="mdb-strict-max-pool")
/subsystem=ejb3:write-attribute(name="default-resource-adapter-name", value=$resourceAdapterName)
# ---------------------------------------------

# ---------------------------------------------
# Adding admin-objects

